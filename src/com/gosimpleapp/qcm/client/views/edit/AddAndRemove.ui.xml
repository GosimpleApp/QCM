<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.sized {font-size: 35px;}
	.bold_sized {font-size: 35px;font-weight:bold;}
	</ui:style>
	<g:HorizontalPanel  spacing="5">
		<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:PushButton ui:field='removePushButton' enabled='true' width="21px" height="32px">
  		 <g:upFace>
			<img src="images/glyphicons_016_bin.png"/>
   		</g:upFace>
 	</g:PushButton>
		</g:Cell>
	<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
		<g:PushButton ui:field='newPushButton' enabled='true' width="21px" height="32px">
  		 <g:upFace>
			<img src="images/glyphicons_151_new_window.png"/>
   		</g:upFace>
 	</g:PushButton>
	</g:Cell>
 	
	</g:HorizontalPanel>
</ui:UiBinder> 