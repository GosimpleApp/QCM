<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style >
	</ui:style>



	<g:LayoutPanel>
		<g:layer height="10%" top="0%">
			<g:Label ui:field="question" text="question"
				horizontalAlignment="ALIGN_CENTER" styleName="question" />
		</g:layer>

	

		<g:layer height="40%" top="10%">
			<g:LayoutPanel>
			
				<g:layer height="48%" top="1%" width="48%" left="1%">
					<g:LayoutPanel>
						<g:layer >
					
							<g:DecoratorPanel ui:field="panel_0" width="100%" height="100%">
							
								<g:Label text="proposal_0" ui:field="proposal_0" horizontalAlignment="ALIGN_CENTER" styleName="proposal" > </g:Label>
							</g:DecoratorPanel>
						</g:layer>
					</g:LayoutPanel>
				</g:layer>
				
				<g:layer height="48%" top="1%" width="48%" left="50%">
					<g:LayoutPanel>
						<g:layer>
							<g:DecoratorPanel ui:field="panel_1"  width="100%" height="100%">
								<g:Label text="proposal_1" horizontalAlignment="ALIGN_CENTER"  ui:field="proposal_1" styleName="proposal"></g:Label>
							</g:DecoratorPanel>
						</g:layer>
					</g:LayoutPanel>
				</g:layer>

				<g:layer height="48%" top="50%" width="48%" left="1%">
					<g:LayoutPanel>
						<g:layer>
							<g:DecoratorPanel ui:field="panel_2"  width="100%" height="100%">
									<g:Label text="proposal_2" ui:field="proposal_2" horizontalAlignment="ALIGN_CENTER" styleName="proposal"></g:Label>
							</g:DecoratorPanel>
						</g:layer>
					</g:LayoutPanel>
				</g:layer>
				<g:layer height="48%" top="50%" width="48%" left="50%">
					<g:LayoutPanel>
						<g:layer>
							<g:DecoratorPanel ui:field="panel_3"  width="100%" height="100%">
								<g:Label  text="proposal_3" ui:field="proposal_3" horizontalAlignment="ALIGN_CENTER" styleName="proposal" ></g:Label>
							</g:DecoratorPanel>
						</g:layer>
					</g:LayoutPanel>
				</g:layer>

			</g:LayoutPanel>
		</g:layer>
		
			<g:layer height="10%" top="50%">
			<g:HTML horizontalAlignment="ALIGN_CENTER" ui:field="explanation" styleName="explanation">explanation"
			</g:HTML>
		</g:layer>
		
		<g:layer height="10%" top="60%" width="50%" left="25%">
					<g:Button ui:field="next" width="100%" styleName="next">Question suivante</g:Button>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 